{"ast":null,"code":"import { ref, reactive, watch, watchEffect } from \"vue\";\nexport default {\n  name: \"App\",\n  setup() {\n    // 数据\n    let sum = ref(0);\n    let msg = ref('你好!');\n    let person = reactive({\n      name: \"张三\",\n      age: 18,\n      job: {\n        j1: {\n          salay: 20\n        }\n      }\n    });\n    // 监视\n    watch(sum, (newValue, oldValue) => {\n      console.log('sum的值变化了', '新:', newValue, '旧:', oldValue);\n    }, {\n      immediate: true\n    });\n    watchEffect(() => {\n      const x1 = sum.value;\n      const x2 = person.job.j1;\n      console.log('watchEffect所指定的回调执行了', x1);\n    });\n    return {\n      sum,\n      msg,\n      person\n    };\n  }\n  // vue2中的watch\n  //   watch:{\n  //      // 简写\n  //     sum(newValue,oldValue){\n  //         console.log('sum的值变化了','新:',newValue,'旧:',oldValue)\n  //     }\n  //      // 完整写法\n  //     sum:{\n  //         immediate:true,\n  //         deep:true,\n  //         handler(newValue,oldValue){\n  //             console.log('sum的值变化了','新:',newValue,'旧:',oldValue)\n  //         }\n  //     }\n  //   },\n};","map":{"version":3,"mappings":"AAgBA,SAASA,GAAG,EAACC,QAAQ,EAACC,KAAK,EAACC,WAAU,QAAS,KAAK;AACpD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,KAAK,GAAG;IACN;IACA,IAAIC,GAAE,GAAIN,GAAG,CAAC,CAAC,CAAC;IAChB,IAAIO,GAAG,GAACP,GAAG,CAAC,KAAK;IACjB,IAAIQ,MAAM,GAACP,QAAQ,CAAC;MAChBG,IAAI,EAAC,IAAI;MACTK,GAAG,EAAC,EAAE;MACNC,GAAG,EAAC;QACAC,EAAE,EAAC;UACCC,KAAK,EAAC;QACV;MACJ;IACJ,CAAC;IACD;IACAV,KAAK,CAACI,GAAG,EAAC,CAACO,QAAQ,EAACC,QAAQ,KAAG;MAC3BC,OAAO,CAACC,GAAG,CAAC,UAAU,EAAC,IAAI,EAACH,QAAQ,EAAC,IAAI,EAACC,QAAQ;IACtD,CAAC,EAAC;MAACG,SAAS,EAAC;IAAI,CAAC;IAElBd,WAAW,CAAC,MAAI;MACZ,MAAMe,EAAE,GAACZ,GAAG,CAACa,KAAI;MACjB,MAAMC,EAAE,GAACZ,MAAM,CAACE,GAAG,CAACC,EAAC;MACrBI,OAAO,CAACC,GAAG,CAAC,sBAAsB,EAACE,EAAE;IACzC,CAAC;IAED,OAAO;MACLZ,GAAG;MACHC,GAAG;MACHC;IACF,CAAC;EACH;EACF;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;AACA,CAAC","names":["ref","reactive","watch","watchEffect","name","setup","sum","msg","person","age","job","j1","salay","newValue","oldValue","console","log","immediate","x1","value","x2"],"sourceRoot":"","sources":["/Users/erlei-c/Desktop/Vue-note/vue3/src/components/Demo.vue"],"sourcesContent":["<template>\n  <h2>当前求和为:{{ sum }}</h2>\n  <button @click=\"sum++\">点我加一</button>\n  <hr>\n  <h2>当前的信息为:{{msg}}</h2>\n  <button @click=\"msg+='!'\">修改信息</button>\n  <hr>\n  <h2>姓名:{{person.name}}</h2>\n  <h2>年龄:{{person.age}}</h2>\n  <h2>薪水:{{person.job.j1.salay}}k</h2>\n  <button @click=\"person.name+='~'\">修改姓名</button>\n  <button @click=\"person.age++\">修改年龄</button>\n  <button @click=\"person.job.j1.salay++\">涨薪</button>\n</template>\n  \n  <script>\nimport { ref,reactive,watch,watchEffect } from \"vue\";\nexport default {\n  name: \"App\",\n  setup() {\n    // 数据\n    let sum = ref(0);\n    let msg=ref('你好!')\n    let person=reactive({\n        name:\"张三\",\n        age:18,\n        job:{\n            j1:{\n                salay:20\n            }\n        }\n    })\n    // 监视\n    watch(sum,(newValue,oldValue)=>{\n        console.log('sum的值变化了','新:',newValue,'旧:',oldValue)\n    },{immediate:true})\n    \n    watchEffect(()=>{\n        const x1=sum.value\n        const x2=person.job.j1\n        console.log('watchEffect所指定的回调执行了',x1)\n    })\n\n    return {\n      sum,\n      msg,\n      person\n    };\n  },\n// vue2中的watch\n//   watch:{\n//      // 简写\n//     sum(newValue,oldValue){\n//         console.log('sum的值变化了','新:',newValue,'旧:',oldValue)\n//     }\n//      // 完整写法\n//     sum:{\n//         immediate:true,\n//         deep:true,\n//         handler(newValue,oldValue){\n//             console.log('sum的值变化了','新:',newValue,'旧:',oldValue)\n//         }\n//     }\n//   },\n};\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}