{"ast":null,"code":"import { reactive, toRefs, ref, toRaw } from \"vue\";\nexport default {\n  name: \"App\",\n  setup() {\n    // 数据\n    let sum = ref(0);\n    let person = reactive({\n      name: \"张三\",\n      age: 18,\n      job: {\n        j1: {\n          salay: 20\n        }\n      }\n    });\n    function showRawPerson() {\n      let p = toRaw(person);\n      console.log(p);\n    }\n    function addCar() {\n      let car = {\n        name: \"奔驰\",\n        price: '40W'\n      };\n      person.car = car;\n    }\n    return {\n      sum,\n      person,\n      ...toRefs(person),\n      showRawPerson,\n      addCar\n    };\n  }\n};","map":{"version":3,"mappings":"AAkBA,SAASA,QAAQ,EAAEC,MAAM,EAAEC,GAAG,EAACC,KAAK,QAAS,KAAK;AAClD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,KAAK,GAAG;IACN;IACA,IAAIC,GAAE,GAAGJ,GAAG,CAAC,CAAC;IACd,IAAIK,MAAK,GAAIP,QAAQ,CAAC;MACpBI,IAAI,EAAE,IAAI;MACVI,GAAG,EAAE,EAAE;MACPC,GAAG,EAAE;QACHC,EAAE,EAAE;UACFC,KAAK,EAAE;QACT;MACF;IACF,CAAC,CAAC;IAEF,SAASC,aAAa,GAAE;MACrB,IAAIC,CAAC,GAAEV,KAAK,CAACI,MAAM;MACnBO,OAAO,CAACC,GAAG,CAACF,CAAC;IAChB;IACA,SAASG,MAAM,GAAE;MACb,IAAIC,GAAG,GAAC;QAACb,IAAI,EAAC,IAAI;QAACc,KAAK,EAAC;MAAK;MAC9BX,MAAM,CAACU,GAAG,GAACA,GAAE;IACjB;IACA,OAAO;MACLX,GAAG;MACHC,MAAM;MACN,GAAGN,MAAM,CAACM,MAAM,CAAC;MACjBK,aAAa;MACbI;IACF,CAAC;EACH;AACF,CAAC","names":["reactive","toRefs","ref","toRaw","name","setup","sum","person","age","job","j1","salay","showRawPerson","p","console","log","addCar","car","price"],"sourceRoot":"","sources":["/Users/erlei-c/Desktop/Vue-note/vue3/src/components/Demo.vue"],"sourcesContent":["<template>\n  <h4>当前的求和是:{{ sum }}</h4>\n  <button @click=\"sum++\">点我sum++</button>\n  <hr />\n  <h2>姓名:{{ name }}</h2>\n  <h2>年龄:{{ age }}</h2>\n  <h2>薪水:{{ job.j1.salay }}k</h2>\n  <h2 v-show=\"person.car\">汽车信息:{{ person.car }}</h2>\n  <button @click=\"name += '~'\">修改姓名</button>\n  <button @click=\"age++\">修改年龄</button>\n  <button @click=\"job.j1.salay++\">涨薪</button>\n  <button @click=\"showRawPerson\">输出最原始的perosn</button>\n  <button @click=\"addCar\">给人添加一台车</button>\n  <button @click=\"addCar\">换车名</button>\n  <button @click=\"addCar\">给人添加一台车</button>\n</template>\n\n<script>\nimport { reactive, toRefs, ref,toRaw, } from \"vue\";\nexport default {\n  name: \"App\",\n  setup() {\n    // 数据\n    let sum =ref(0)\n    let person = reactive({\n      name: \"张三\",\n      age: 18,\n      job: {\n        j1: {\n          salay: 20,\n        },\n      },\n    });\n\n    function showRawPerson(){\n       let p= toRaw(person)\n       console.log(p)\n    }\n    function addCar(){\n        let car={name:\"奔驰\",price:'40W'}\n        person.car=car\n    }\n    return {\n      sum,\n      person,\n      ...toRefs(person),\n      showRawPerson,\n      addCar\n    };\n  },\n};\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}